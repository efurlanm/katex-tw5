/* KaTeX styles */

{{$:/plugins/orange/katex-tw5/katex.min.css}}

/* Force text-rendering  (see https://github.com/Jermolene/TiddlyWiki5/issues/2500) */

.katex {
	text-rendering: auto;
}

/* Reset Automatic Numbering on a per tiddler basis */

.tc-tiddler-frame {
	counter-reset: katexEqnNo;
}

/* Avoid TW5's max-width: 100% */

.katex svg {
	max-width: initial;
}

/* Override font URLs */

@font-face {
	font-family: KaTeX_AMS;
	src: url(<<datauri '$:/plugins/orange/katex-tw5/fonts/KaTeX_AMS-Regular.woff2'>>) format('woff2');
	font-weight: normal;
	font-style: normal;
}
